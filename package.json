{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint:next": "next lint",
    "lint:types": "tsc --noEmit --pretty",
    "lint:format": "prettier --check --config .prettierrc.js --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:fix": "pnpm lint:types && pnpm lint:next --fix && pnpm lint:format --write",
    "lint": "pnpm lint:types && pnpm lint:next && pnpm lint:format",
    "precommit": "pnpm lint-staged"
  },
  "dependencies": {
    "next": "12.1.6",
    "next-themes": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.15.4",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.3",
    "tsc-files": "^1.1.3",
    "typescript": "4.7.3"
  }
}
