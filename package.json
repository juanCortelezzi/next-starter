{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:next": "next lint",
    "lint:types": "tsc --noEmit --pretty",
    "lint:format": "prettier --check --config .prettierrc.js --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:fix": "pnpm lint:types && pnpm lint:next --fix && pnpm lint:format --write",
